class Cell {
    field int row;
    field int column;

    constructor Cell new(int r, int c) {
        let row = r;
        let column = c;

        return this;
    }

    method int getRow() {
        return row;
    }

    method int getColumn() {
        return column;
    }

    method void setRow(int r) {
        let row = r;
        return;
    }

    method void setColumn(int c) {
        let column = c;
        return;
    }
}